--
-- Table structure for table `empleado`
--

DROP TABLE IF EXISTS `empleado`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `empleado` (
  `idEmpleado` tinyint NOT NULL AUTO_INCREMENT,
  `idarea` tinyint NOT NULL,
  `cargo_idcargo` tinyint NOT NULL,
  `idarl` tinyint NOT NULL,
  `ideps` tinyint NOT NULL,
  `idafp` tinyint NOT NULL,
  `id_tipo_identificacion` tinyint(1) NOT NULL,
  `idmunicipios` int NOT NULL,
  `nombre_empleado` varchar(45) NOT NULL,
  `idperfil` tinyint NOT NULL,
  `apellido_empleado` varchar(45) NOT NULL,
  `identificacion_empleado` int NOT NULL,
  `telefono_empleado` varchar(20) DEFAULT NULL,
  `direccion_empleado` varchar(120) NOT NULL,
  `fecha_nacimiento` date DEFAULT NULL,
  `coreo_empleado` varchar(45) DEFAULT NULL,
  `telefono_corporativo` varchar(20) DEFAULT NULL,
  `estado` enum('Activo','Inactivo') DEFAULT NULL,
  `fecha_ingreso` date NOT NULL,
  `contrasena` varchar(70) NOT NULL,
  PRIMARY KEY (`idEmpleado`),
  UNIQUE KEY `uq_identificacion_empleado` (`identificacion_empleado`),
  UNIQUE KEY `uq_coreo_empleado` (`coreo_empleado`),
  KEY `fk_Empleado_municipios1_idx` (`idmunicipios`),
  KEY `fk_Empleado_cargo1_idx` (`cargo_idcargo`),
  KEY `fk_Empleado_tipo_identificacion1_idx` (`id_tipo_identificacion`),
  KEY `fk_Empleado_area1_idx` (`idarea`),
  KEY `fk_Empleado_eps1_idx` (`ideps`),
  KEY `fk_Empleado_arl1_idx` (`idarl`),
  KEY `fk_Empleado_afp1_idx` (`idafp`),
  KEY `fk_Empleado_perfil1_idx` (`idperfil`),
  CONSTRAINT `fk_Empleado_afp1` FOREIGN KEY (`idafp`) REFERENCES `afp` (`idafp`),
  CONSTRAINT `fk_Empleado_area1` FOREIGN KEY (`idarea`) REFERENCES `area` (`idarea`),
  CONSTRAINT `fk_Empleado_arl1` FOREIGN KEY (`idarl`) REFERENCES `arl` (`idarl`),
  CONSTRAINT `fk_Empleado_cargo1` FOREIGN KEY (`cargo_idcargo`) REFERENCES `cargo` (`idcargo`),
  CONSTRAINT `fk_Empleado_eps1` FOREIGN KEY (`ideps`) REFERENCES `eps` (`ideps`),
  CONSTRAINT `fk_Empleado_municipios1` FOREIGN KEY (`idmunicipios`) REFERENCES `municipios` (`idmunicipios`),
  CONSTRAINT `fk_Empleado_perfil1` FOREIGN KEY (`idperfil`) REFERENCES `perfil` (`idperfil`),
  CONSTRAINT `fk_Empleado_tipo_identificacion1` FOREIGN KEY (`id_tipo_identificacion`) REFERENCES `tipo_identificacion` (`id_tipo_identificacion`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8mb3;
/*!40101 SET character_set_client = @saved_cs_client */;


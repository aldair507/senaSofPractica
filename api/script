--
-- Table structure for table `sedes_cliente`
--

DROP TABLE IF EXISTS `sedes_cliente`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `sedes_cliente` (
  `idsedes_cliente` tinyint NOT NULL,
  `idcliente` tinyint NOT NULL,
  `idmunicipios` int NOT NULL,
  `nombre_sede` varchar(130) NOT NULL,
  `direccion_sede` varchar(75) DEFAULT NULL,
  `telefono_sede` varchar(20) DEFAULT NULL,
  `frecuencia` enum('Diaria','Una vez por semana','Una vez por mes') DEFAULT NULL,
  `fecha_registro_sede` date DEFAULT NULL,
  `codigo_sede` varchar(20) DEFAULT NULL,
  PRIMARY KEY (`idsedes_cliente`),
  KEY `fk_sedes_cliente_cliente1_idx` (`idcliente`),
  KEY `fk_sedes_cliente_municipios1_idx` (`idmunicipios`),
  CONSTRAINT `fk_sedes_cliente_cliente1` FOREIGN KEY (`idcliente`) REFERENCES `cliente` (`idcliente`),
  CONSTRAINT `fk_sedes_cliente_municipios1` FOREIGN KEY (`idmunicipios`) REFERENCES `municipios` (`idmunicipios`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3;
/*!40101 SET character_set_client = @saved_cs_client */;


CREATE TABLE `cliente` (
  `idcliente` tinyint NOT NULL AUTO_INCREMENT,
  `idclasificacion_cliente` tinyint NOT NULL,
  `idestado_cliente` tinyint NOT NULL,
  `idmunicipios` int NOT NULL,
  `id_tipo_identificacion` tinyint(1) NOT NULL,
  `razon_social` varchar(130) NOT NULL,
  `nit` varchar(20) DEFAULT NULL,
  `digito_verificacion` tinyint NOT NULL,
  `nombres_representante_legal` varchar(45) NOT NULL,
  `apellidos_representante_legal` varchar(45) NOT NULL,
  `identificacion_representantel` int DEFAULT NULL,
  `telefono_representantel` varchar(20) DEFAULT NULL,
  `correo_representantel` varchar(45) DEFAULT NULL,
  `persona_contacto` varchar(45) DEFAULT NULL,
  `telefono_persona_contacto` varchar(20) DEFAULT NULL,
  `correo_persona_contacto` varchar(45) DEFAULT NULL,
  `observaciones` varchar(130) DEFAULT NULL,
  `frecuencia` enum('Diaria','Una vez por semana','Una vez por mes') NOT NULL,
  `metodo_facturacion` enum('Cliente','Sede') DEFAULT NULL,
  `fecha_registro` date DEFAULT NULL,
  PRIMARY KEY (`idcliente`),
  UNIQUE KEY `unique_identificacion_representantel` (`identificacion_representantel`),
  KEY `fk_cliente_clasificacion_cliente1_idx` (`idclasificacion_cliente`),
  KEY `fk_cliente_estado_cliente1_idx` (`idestado_cliente`),
  KEY `fk_cliente_municipios1_idx` (`idmunicipios`),
  KEY `fk_cliente_tipo_identificacion1_idx` (`id_tipo_identificacion`),
  CONSTRAINT `fk_cliente_clasificacion_cliente1` FOREIGN KEY (`idclasificacion_cliente`) REFERENCES `clasificacion_cliente` (`idclasificacion_cliente`),
  CONSTRAINT `fk_cliente_estado_cliente1` FOREIGN KEY (`idestado_cliente`) REFERENCES `estado_cliente` (`idestado_cliente`),
  CONSTRAINT `fk_cliente_municipios1` FOREIGN KEY (`idmunicipios`) REFERENCES `municipios` (`idmunicipios`),
  CONSTRAINT `fk_cliente_tipo_identificacion1` FOREIGN KEY (`id_tipo_identificacion`) REFERENCES `tipo_identificacion` (`id_tipo_identificacion`)
) ENGINE=InnoDB AUTO_INCREMENT=106 DEFAULT CHARSET=utf8mb3;
/*!40101 SET character_set_client = @saved_cs_client */;

